<h1>New image</h1>

<% form_for :image,:html=>{:multipart =>true} do |f| %>
  <%= f.error_messages %>

   <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>

  <p>
    <%= f.label :uploader %><br />
    <%= f.text_field :uploader %>
  </p>
  
  <p>
    <%=f.file_field :picture%>
  </p>

  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', images_path %>
